<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Le_Panier.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vues="clr-namespace:Le_Panier.Vues.Register"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    xmlns:local="clr-namespace:Le_Panier"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="True"
    Title="Le_Panier">
    <Shell.Behaviors>
        <toolkit:StatusBarBehavior StatusBarColor="#E65100"/>
    </Shell.Behaviors>
    
    <ShellContent
    Title="Register1"
    ContentTemplate="{DataTemplate vues:Etape1}"
    Route="Etape1" />
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate vues:Etape2}"
        Route="MainPage" />
 

</Shell>
